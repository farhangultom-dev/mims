<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginStart="25dp"
    android:layout_marginTop="15dp"
    android:layout_marginEnd="25dp"
    android:background="#F7F8FA"
    app:cardCornerRadius="5dp">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <View
            android:id="@+id/view"
            android:layout_width="10dp"
            android:layout_height="0dp"
            android:background="#26ACE2"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/lbl_no_permintaan"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="15dp"
            android:layout_marginTop="8dp"
            android:fontFamily="@font/montserrat_reguler"
            android:text="No. Permintaan"
            android:textSize="10sp"
            app:layout_constraintEnd_toStartOf="@id/lbl_no_do"
            app:layout_constraintStart_toEndOf="@id/view"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/lbl_no_do"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="15dp"
            android:layout_marginTop="10dp"
            android:fontFamily="@font/montserrat_reguler"
            android:text="No. Transaksi"
            android:textSize="10sp"
            app:layout_constraintEnd_toStartOf="@+id/btn_detail"
            app:layout_constraintStart_toEndOf="@id/lbl_no_permintaan"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/btn_detail"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="15dp"
            android:src="@drawable/ic_src_doc"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/txt_no_permintaan"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:fontFamily="@font/montserrat_semibold"
            android:text="10011"
            android:textColor="#3F3D56"
            android:textSize="12sp"
            app:layout_constraintEnd_toEndOf="@+id/lbl_no_permintaan"
            app:layout_constraintStart_toStartOf="@+id/lbl_no_permintaan"
            app:layout_constraintTop_toBottomOf="@id/lbl_no_permintaan" />

        <TextView
            android:id="@+id/txt_no_transaksi"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:fontFamily="@font/montserrat_semibold"
            android:text="10011"
            android:textColor="#3F3D56"
            android:textSize="12sp"
            app:layout_constraintEnd_toEndOf="@+id/lbl_no_do"
            app:layout_constraintStart_toStartOf="@+id/lbl_no_do"
            app:layout_constraintTop_toBottomOf="@id/lbl_no_do" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="14dp"
            android:layout_height="14dp"
            android:layout_marginTop="12dp"
            android:src="@drawable/ic_number"
            app:layout_constraintStart_toStartOf="@+id/txt_no_permintaan"
            app:layout_constraintTop_toBottomOf="@+id/txt_tgl_permintaan" />

        <TextView
            android:id="@+id/lbl_gudang_asal"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:fontFamily="@font/montserrat_reguler"
            android:text="Gudang Asal"
            android:textColor="#3F3D56"
            android:textSize="10sp"
            app:layout_constraintEnd_toEndOf="@+id/txt_no_permintaan"
            app:layout_constraintStart_toEndOf="@+id/imageView2"
            app:layout_constraintTop_toTopOf="@+id/imageView2" />

        <TextView
            android:id="@+id/txt_gudang_asal"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:layout_marginBottom="10dp"
            android:fontFamily="@font/montserrat_reguler"
            android:text="1110011"
            android:textSize="10sp"
            app:layout_constraintEnd_toEndOf="@+id/txt_no_permintaan"
            app:layout_constraintStart_toEndOf="@+id/imageView2"
            app:layout_constraintTop_toBottomOf="@+id/lbl_gudang_asal" />

        <ImageView
            android:id="@+id/imageViewjumlahKardus"
            android:layout_width="14dp"
            android:layout_height="14dp"
            android:layout_marginTop="12dp"
            android:src="@drawable/ic_number"
            app:layout_constraintStart_toStartOf="@+id/txt_no_permintaan"
            app:layout_constraintTop_toBottomOf="@+id/txt_gudang_asal" />

        <TextView
            android:id="@+id/lbl_jumlah_kardus"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:fontFamily="@font/montserrat_reguler"
            android:text="Total Package"
            android:textColor="#3F3D56"
            android:textSize="10sp"
            app:layout_constraintEnd_toEndOf="@+id/txt_no_permintaan"
            app:layout_constraintStart_toEndOf="@+id/imageViewjumlahKardus"
            app:layout_constraintTop_toTopOf="@+id/imageViewjumlahKardus" />

        <TextView
            android:id="@+id/txt_jumlah_kardus"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:layout_marginBottom="10dp"
            android:fontFamily="@font/montserrat_reguler"
            android:text="1110011"
            android:textSize="10sp"
            app:layout_constraintEnd_toEndOf="@+id/txt_no_permintaan"
            app:layout_constraintStart_toEndOf="@+id/imageView2"
            app:layout_constraintTop_toBottomOf="@+id/lbl_jumlah_kardus" />

        <ImageView
            android:id="@+id/imageViewNoPengiriman"
            android:layout_width="14dp"
            android:layout_height="14dp"
            android:layout_marginTop="12dp"
            android:src="@drawable/ic_number"
            app:layout_constraintStart_toStartOf="@+id/txt_no_permintaan"
            app:layout_constraintTop_toBottomOf="@+id/txt_jumlah_kardus" />

        <TextView
            android:id="@+id/lbl_no_pengiriman"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:fontFamily="@font/montserrat_reguler"
            android:text="No Pengiriman"
            android:textColor="#3F3D56"
            android:textSize="10sp"
            app:layout_constraintEnd_toEndOf="@+id/txt_no_permintaan"
            app:layout_constraintStart_toEndOf="@+id/imageViewNoPengiriman"
            app:layout_constraintTop_toTopOf="@+id/imageViewNoPengiriman" />

        <TextView
            android:id="@+id/txt_no_pengiriman"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:layout_marginBottom="10dp"
            android:fontFamily="@font/montserrat_reguler"
            android:text="1110011"
            android:textSize="10sp"
            app:layout_constraintEnd_toEndOf="@+id/txt_no_permintaan"
            app:layout_constraintStart_toEndOf="@+id/imageView2"
            app:layout_constraintTop_toBottomOf="@+id/lbl_no_pengiriman" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="14dp"
            android:layout_height="14dp"
            android:layout_marginTop="12dp"
            android:src="@drawable/ic_number"
            app:layout_constraintStart_toStartOf="@+id/txt_no_transaksi"
            app:layout_constraintTop_toBottomOf="@+id/txt_gudang_tujuan" />

        <TextView
            android:id="@+id/lbl_status_pengeluaran"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:fontFamily="@font/montserrat_reguler"
            android:text="Status"
            android:textColor="#3F3D56"
            android:textSize="10sp"
            app:layout_constraintBottom_toBottomOf="@+id/imageView3"
            app:layout_constraintEnd_toEndOf="@+id/txt_no_transaksi"
            app:layout_constraintStart_toEndOf="@+id/imageView3"
            app:layout_constraintTop_toTopOf="@+id/imageView3" />

        <TextView
            android:id="@+id/txt_status_pengeluaran"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="5dp"
            android:fontFamily="@font/montserrat_reguler"
            android:text="21-01-2023"
            android:textSize="10sp"
            app:layout_constraintBottom_toBottomOf="@+id/txt_gudang_asal"
            app:layout_constraintEnd_toEndOf="@+id/txt_no_transaksi"
            app:layout_constraintStart_toEndOf="@+id/imageView3"
            app:layout_constraintTop_toBottomOf="@+id/lbl_status_pengeluaran" />

        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="14dp"
            android:layout_height="14dp"
            android:layout_marginTop="12dp"
            android:src="@drawable/ic_number"
            app:layout_constraintStart_toStartOf="@+id/txt_no_transaksi"
            app:layout_constraintTop_toBottomOf="@+id/txt_status_pengeluaran" />

        <TextView
            android:id="@+id/lbl_qty_permintaan"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:fontFamily="@font/montserrat_reguler"
            android:text="QTY Permintaan"
            android:textColor="#3F3D56"
            android:textSize="10sp"
            app:layout_constraintBottom_toBottomOf="@+id/imageView7"
            app:layout_constraintEnd_toEndOf="@+id/txt_no_transaksi"
            app:layout_constraintStart_toEndOf="@+id/imageView7"
            app:layout_constraintTop_toTopOf="@+id/imageView7" />

        <TextView
            android:id="@+id/txt_qty_permintaan"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/montserrat_reguler"
            android:text="21-01-2023"
            android:textSize="10sp"
            app:layout_constraintEnd_toEndOf="@id/lbl_qty_permintaan"
            app:layout_constraintStart_toStartOf="@id/lbl_qty_permintaan"
            app:layout_constraintTop_toBottomOf="@id/lbl_qty_permintaan" />

        <ImageView
            android:id="@+id/imageView8"
            android:layout_width="14dp"
            android:layout_height="14dp"
            android:layout_marginTop="12dp"
            android:src="@drawable/ic_number"
            app:layout_constraintStart_toStartOf="@+id/txt_no_transaksi"
            app:layout_constraintTop_toBottomOf="@+id/txt_qty_permintaan" />

        <TextView
            android:id="@+id/lbl_qty_scanned"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:fontFamily="@font/montserrat_reguler"
            android:text="Total Scan"
            android:textColor="#3F3D56"
            android:textSize="10sp"
            app:layout_constraintBottom_toBottomOf="@+id/imageView8"
            app:layout_constraintEnd_toEndOf="@+id/txt_no_transaksi"
            app:layout_constraintStart_toEndOf="@+id/imageView8"
            app:layout_constraintTop_toTopOf="@+id/imageView8" />

        <TextView
            android:id="@+id/txt_qty_scanned"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp"
            android:fontFamily="@font/montserrat_reguler"
            android:text="21-01-2023"
            android:textSize="10sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@id/lbl_qty_scanned"
            app:layout_constraintStart_toStartOf="@id/lbl_qty_scanned"
            app:layout_constraintTop_toBottomOf="@id/lbl_qty_scanned" />

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="14dp"
            android:layout_height="14dp"
            android:layout_marginTop="10dp"
            android:src="@drawable/ic_number"
            app:layout_constraintStart_toStartOf="@+id/txt_no_permintaan"
            app:layout_constraintTop_toBottomOf="@+id/txt_no_permintaan" />

        <TextView
            android:id="@+id/lbl_tgl_permintaan"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:fontFamily="@font/montserrat_reguler"
            android:text="Tanggal Permintaan"
            android:textColor="#3F3D56"
            android:textSize="10sp"
            app:layout_constraintBottom_toBottomOf="@+id/imageView4"
            app:layout_constraintEnd_toEndOf="@+id/txt_no_permintaan"
            app:layout_constraintStart_toEndOf="@+id/imageView4"
            app:layout_constraintTop_toTopOf="@+id/imageView4" />

        <TextView
            android:id="@+id/txt_tgl_permintaan"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:fontFamily="@font/montserrat_reguler"
            android:text="111011"
            android:textSize="10sp"
            app:layout_constraintEnd_toEndOf="@+id/lbl_tgl_permintaan"
            app:layout_constraintStart_toEndOf="@+id/imageView4"
            app:layout_constraintTop_toBottomOf="@+id/lbl_tgl_permintaan" />

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="14dp"
            android:layout_height="14dp"
            android:layout_marginTop="10dp"
            android:src="@drawable/ic_number"
            app:layout_constraintStart_toStartOf="@+id/txt_no_transaksi"
            app:layout_constraintTop_toBottomOf="@+id/txt_no_transaksi" />

        <TextView
            android:id="@+id/lbl_gudang_tujuan"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:fontFamily="@font/montserrat_reguler"
            android:text="Gudang Tujuan"
            android:textColor="#3F3D56"
            android:textSize="10sp"
            app:layout_constraintEnd_toEndOf="@+id/txt_no_transaksi"
            app:layout_constraintStart_toEndOf="@+id/imageView5"
            app:layout_constraintTop_toTopOf="@+id/imageView5" />

        <TextView
            android:id="@+id/txt_gudang_tujuan"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:fontFamily="@font/montserrat_reguler"
            android:text="1"
            android:textSize="10sp"
            app:layout_constraintEnd_toEndOf="@+id/lbl_gudang_tujuan"
            app:layout_constraintStart_toEndOf="@+id/imageView5"
            app:layout_constraintTop_toBottomOf="@+id/lbl_gudang_tujuan" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraint_fog"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#39000000">

        <TextView
            android:id="@+id/tv_click_get_data"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/montserrat_bold"
            android:text="Klik untuk menarik data pengiriman ini"
            android:textColor="@color/black"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/tv_persentase_data"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_10sdp"
            android:layout_marginEnd="@dimen/_10sdp"
            android:fontFamily="@font/montserrat_bold"
            android:text="Data : 0%"
            android:textColor="@color/black"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.cardview.widget.CardView>